{
  "cli": {
    "version": ">= 14.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "releaseChannel": "production",
        "env": {
          "JAVA_HOME": "C:/Program Files/Java/jdk-23"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
